{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "lib": ["dom", "es2015"],
    "outDir": "./static/frontend/",
    "sourceMap": true,
    "baseUrl": "./src",
    "paths": {
      "~*": ["./*"]
    },
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react"
  }
}
